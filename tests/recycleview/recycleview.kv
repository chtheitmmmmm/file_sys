<RecycleViewWidget>:  ##定义页面
    viewclass: 'Label'   ##视图类
    RecycleBoxLayout:  ##遍历数据
        default_size: None, dp(56)  ##设置默认大小
        default_size_hint: 1, None  ##设置默认大小
        size_hint_y: None  ##设置大小
        height: self.minimum_height    ##设置高度
        orientation: 'vertical'  ##指定布局方向
